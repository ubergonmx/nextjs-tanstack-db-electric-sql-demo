# Better Auth secret (generate with: openssl rand -base64 32)
BETTER_AUTH_SECRET=

# App URL for Better Auth
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Neon Database owner connection string
DATABASE_URL=

# Electric SQL Cloud configuration
ELECTRIC_SQL_CLOUD_SOURCE_ID=
ELECTRIC_SQL_CLOUD_SOURCE_SECRET=

# API endpoint secret for secure contact creation
# Only needed if you want to call `api/contacts/create` from a third-party service to emulate a background/third-party data update.
API_SECRET=

# Web Push VAPID keys (generate with: npx web-push generate-vapid-keys)
NEXT_PUBLIC_VAPID_PUBLIC_KEY=
VAPID_PRIVATE_KEY=
